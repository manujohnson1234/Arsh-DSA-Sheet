class Solution {
public:
    int maxScore(vector<int>& v, int k) {
        int points = 0;
        for(int i =0; i < k; i++){
            points += v[i];
        }

        int ans = points;

        int r = v.size()-1;

        for(int i =k-1; i >= 0; i--){
            points -= v[i];
            points += v[r--];
            ans = max(ans, points);
        }

        return ans;
    }
};
