class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        vector<int> ans;
        for(int i =0; i < nums.size(); i++){
            int v = abs(nums[i]);
            if(nums[v-1] < 0){
                ans.push_back(v);
            }else{
                nums[v-1] *= -1;
            }

        }
        return ans;
    }
};
