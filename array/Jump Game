class Solution {
public:
    bool canJump(vector<int>& nums) {
        if(nums[0] == 0 && nums.size() > 1) return false;
        int jumps = nums[0];
        for(int i =1; i < nums.size(); i++){
            if(jumps == 0) return false;
            jumps = max(jumps-1, nums[i]);
            
        }
        return true;
    }
};
